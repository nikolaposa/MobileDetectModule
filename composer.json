{
    "name": "jonathanmaron/mobile-detect-module",
    "description": "ZF3 Module which facilitates integration of a PHP MobileDetect class",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf3",
        "zend",
        "module",
        "mobile",
        "mobiledetect"
    ],
    "homepage": "https://github.com/nikolaposa/MobileDetectModule",
    "authors": [
        {
            "name": "Nikola Posa",
            "email": "posa.nikola@gmail.com",
            "homepage": "http://www.nikolaposa.in.rs"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-http": "^2.0",
        "zendframework/zend-view": "^2.0",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-modulemanager": "^2.0",
        "zendframework/zend-servicemanager": "^3.0",
        "mobiledetect/mobiledetectlib": "^2.0"
    },
    "require-dev": {
        "zendframework/zend-config": "*",
        "zendframework/zend-loader": "*",
        "zendframework/zend-stdlib": "*",
        "zendframework/zend-serializer": "*",
        "zendframework/zend-log": "*",
        "zendframework/zend-i18n": "*",
        "zendframework/zend-test": "*",
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-0": {
            "MobileDetectModule": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "MobileDetectModuleTest": "tests/"
        }
    }
}
