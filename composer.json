{
    "name": "nikolaposa/mobile-detect-module",
    "description": "ZF Module which facilitates integration of a PHP MobileDetect library",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf",
        "mobile",
        "mobiledetect"
    ],
    "homepage": "https://github.com/nikolaposa/MobileDetectModule",
    "authors": [
        {
            "name": "Nikola Po≈°a",
            "email": "posa.nikola@gmail.com",
            "homepage": "http://www.nikolaposa.in.rs"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-view": "^2.7",
        "zendframework/zend-http": "^2.5",
        "mobiledetect/mobiledetectlib": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-i18n": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "MobileDetectModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MobileDetectModuleTest\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "cs-fix": "php-cs-fixer fix --config=.php_cs"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
