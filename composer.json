{
    "name": "jonathanmaron/mobile-detect-module",
    "description": "ZF3 Module which facilitates integration of a PHP MobileDetect class",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf3",
        "zend",
        "module",
        "mobile",
        "mobiledetect"
    ],
    "homepage": "https://github.com/nikolaposa/MobileDetectModule",
    "authors": [
        {
            "name": "Nikola Posa",
            "email": "posa.nikola@gmail.com",
            "homepage": "http://www.nikolaposa.in.rs"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-http": "^2.5.4",
        "zendframework/zend-view": "^2.8",
        "zendframework/zend-mvc": "^3.0.1",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-servicemanager": "^3.1",
        "mobiledetect/mobiledetectlib": "^2.0"
    },
    "require-dev": {
        "zendframework/zend-config": "^2.6",
        "zendframework/zend-loader": "^2.5.1",
        "zendframework/zend-stdlib": "^3.0.1",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-i18n": "^2.7.3",
        "zendframework/zend-test": "^3.0.1",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-0": {
            "MobileDetectModule": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "MobileDetectModuleTest": "tests/"
        }
    }
}
